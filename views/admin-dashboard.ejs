<!-- admin-dashboard.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin Dashboard</title>
</head>
<body>
    <h1>Admin Dashboard</h1>
    <table>
        <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Email</th>
                <th>Mobile Number</th>
                <th>Organization</th>
                <th>Experience</th>
                <th>Skills</th>
                <th>Resume</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user => { %>
                <tr>
                    <td><%= user.firstName %></td>
                    <td><%= user.lastName %></td>
                    <td><%= user.email %></td>
                    <td><%= user.mobileNumber %></td>
                    <td><%= user.organization %></td>
                    <td><%= user.experience %></td>
                    <td><%= user.skills %></td>
                    <td><a href="/view-resume/<%= user.resumeDetails.fileName %>">View Resume</a></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>
